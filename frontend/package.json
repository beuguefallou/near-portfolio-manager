{
  "name": "near-portfolio-hackathon",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:reset": "npx prisma migrate reset --force",
    "db:start": "npx prisma generate && npx prisma migrate dev --name init",
    "dev:mainnet": "dotenv -e .env.mainnet -- next dev",
    "dev:testnet": "dotenv -e .env.testnet -- next dev",
    "build:mainnet": "dotenv -e .env.mainnet -- next build",
    "build:testnet": "dotenv -e .env.testnet -- next build",
    "start:mainnet": "dotenv -e .env.mainnet -- next start",
    "start:testnet": "dotenv -e .env.testnet -- next start",
    "lint": "next lint",
    "dev:docker": "docker-compose up"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "^6.4.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@simplewebauthn/browser": "13.0.0-alpha1",
    "@simplewebauthn/server": "13.0.0-alpha1",
    "@solana/web3.js": "^1.95.8",
    "axios": "^1.7.9",
    "bech32": "^2.0.0",
    "bitcoinjs-lib": "^6.1.7",
    "bn.js": "^5.1.1",
    "borsher": "^3.5.0",
    "bs58": "^6.0.0",
    "bs58check": "^4.0.0",
    "chart.js": "^4.4.7",
    "concurrently": "^9.1.2",
    "elliptic": "^6.6.1",
    "ethers": "^6.13.5",
    "hash.js": "^1.1.7",
    "iron-session": "^8.0.4",
    "near-api-js": "^5.0.1",
    "next": "^13.2.4",
    "qrcode.react": "^4.1.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.4.0",
    "tailwindcss": "^3.2.7",
    "tiny-secp256k1": "^2.2.3",
    "tweetnacl": "^1.0.3",
    "uuid": "^11.0.3"
